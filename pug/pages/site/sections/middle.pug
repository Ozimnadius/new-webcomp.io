                        .form__field
                            input(type='tel' name='tel' placeholder='Телефон').input
                        .form__field
                            input(type='email' name='email' placeholder='E-mail').input
                    .form__row
                        .form__field
                            select.select
                                option(disabled=true selected=true) Тип сайта
                                option Вариант 1
                                option Вариант 2
                                option Вариант 3
                        .form__field
                            select.select
