include ../../../common/_mixins
include ../../../common/_settings

section.site-middle

    //- develop
    .site-dev
        //- logo
        .site-dev__logo
            img(src='images/content/dev/devLogo.png').site-dev__img

        //- desc
        .site-dev__desc
            .site-dev__desc-item
                span.site-dev__desc-name Описание:
                span.site-dev__desc-val  Лидер на рынке электронных книг: Россия, СНГ

            .site-dev__desc-item
                span.site-dev__desc-name Цель:
                span.site-dev__desc-val  Проведение SEO кампании с целью повышения известности и узнаваемости бренда


        //- swiper
        .site-dev__swiper
            .proportion.proportion_1006x591
            .site-dev__swiper-wrap
                .swiper-container.site-dev__container
                    .swiper-wrapper
                        each val in [1,2,3]
                            .swiper-slide.site-dev__slide
                                .proportion.proportion_750x470
                                .site-dev__slide-img
                                    //iframe(src='https://ubf-cons.ru/')
                                    img(src='images/content/dev/dev1.jpg').site-dev__slide-pic

                .site-dev__prev
                    .seporator.seporator_prev
                        .seporator__triangle
                        .seporator__line


                .site-dev__next
                    .seporator
                        .seporator__line
                        .seporator__triangle

        //- title
        .site-dev__title Как мы разрабатываем сайты

        //- chart
        .site-dev__chart
            //- img
            //img(src='images/content/dev/devChart.png').site-dev__chart-img

            //-list
            .site-dev__list
                each item in siteDev
                    .site-dev__item
                      .site-dev__item-day= item.day
                      .site-dev__item-desc= item.desc
                      .site-dev__item-arrow
                          +arrow


        //- banner
        .site__banner
            .banner
                .banner__left
                    .banner__title
                        | Хотите сайт
                        br
                        | дешевле?

                .banner__right
                    .banner__text
                        |Готовые решение под вашу
                        br
                        |тематику от
                        b  20 000 Р
                    button.banner__button.button заказать сайт

    //- team
    .site-team
        .site-team__title Наша команда работает для Вас
        .site-team__name Иванов Александр
        .site-team__text Специалист по SEO более 10 лет. Более 50 успешных проектов
        img(src='images/content/team.png').site-team__img

    //- cost
    .site-cost

        //- title
        .site-cost__title Сколько стоит ваш сайт

        //- subtitle
        .site-cost__sub
            |Заполнение брифа онлайн для расчёта точной стоимости
            br
            |или загрузите свое ТЗ для оценки

        //- tabs
        .site-cost__tabs
            div(data-id='1').site-cost__tab.active
                form(action='#' method='post').form
                    .form__row
                        .form__field
